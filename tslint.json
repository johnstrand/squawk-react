{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "space-before-function-paren": false,
        "only-arrow-functions": false,
        "no-implicit-dependencies": [
            true,
            "dev"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "functions": "never"
                }
            }
        ]
    },
    "rulesDirectory": []
}